<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pageLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:maxHeight="800dp"
    android:maxWidth="800dp"
    android:minHeight="800dp"
    android:minWidth="20dp"
    android:visibility="visible"
    tools:context="com.example.uju.coursetracker.MainActivity"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="363dp"
        android:layout_height="49dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="4dp"
        android:text="@string/predictedgrades"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginLeft="11dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="147dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="57dp"
        android:text="@string/course1"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:text="@string/course1"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="179dp"
        android:layout_height="41dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="46dp"
        android:text="@string/course2"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:text="@string/course2"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="475dp"
        android:layout_height="83dp"
        android:layout_marginEnd="64dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="206dp"
        android:text="@string/predictedcgpaphrase"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:text="@string/predictedcgpaphrase" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="263dp"
        android:layout_height="38dp"
        android:layout_marginBottom="42dp"
        android:layout_marginEnd="62dp"
        android:layout_marginStart="113dp"
        android:layout_marginTop="54dp"
        android:background="@android:drawable/editbox_background"
        android:entries="@string/course2"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/spinner2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginRight="62dp"
        android:layout_marginLeft="113dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="247dp"
        android:layout_height="41dp"
        android:layout_marginBottom="317dp"
        android:layout_marginEnd="75dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="42dp"
        android:background="@android:drawable/editbox_background"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        android:layout_marginRight="75dp"
        android:layout_marginLeft="84dp" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="328dp"
        android:layout_height="59dp"
        android:layout_marginBottom="149dp"
        android:layout_marginEnd="136dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="72dp"
        android:background="@android:drawable/editbox_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

</android.support.constraint.ConstraintLayout>
